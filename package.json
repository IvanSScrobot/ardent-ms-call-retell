{
    "name": "retell-processor",
    "version": "1.0.0",
    "description": "Kubernetes-ready microservice for processing survey responses through Retell AI calls",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "dev": "NODE_ENV=development nodemon src/index.js",
        "lint": "eslint src/",
        "test": "jest",
        "docker:build": "docker build -t retell-processor .",
        "docker:run": "docker run --env-file .env -p 3000:3000 retell-processor"
    },
    "keywords": [
        "retell",
        "kubernetes",
        "microservice",
        "postgresql",
        "phone-calls"
    ],
    "author": "Ardent",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "pino": "^8.15.0",
        "pino-pretty": "^10.2.0",
        "retell-sdk": "^2.3.0",
        "@kubernetes/client-node": "^0.20.0",
        "helmet": "^7.0.0",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "eslint": "^8.48.0",
        "jest": "^29.6.4"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}